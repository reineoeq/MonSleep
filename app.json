{
  "expo": {
    "name": "my-monsleep-app",
    "slug": "my-monsleep-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/monsleep-app-logo.png",
    "scheme": "mymonsleepapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.roeq.mymonsleepapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/monsleep-app-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.roeq.mymonsleepapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/monsleep-app-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/monsleep-app-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4cacbcc-ce12-45f6-9ea5-f8b7afe14eff"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/b4cacbcc-ce12-45f6-9ea5-f8b7afe14eff"
    }
  }
}
